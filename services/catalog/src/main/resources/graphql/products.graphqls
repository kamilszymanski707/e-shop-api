enum ProductCategory {
    ELECTRONICS
    AUTOMOTIVE
}

type ProductOutput {
    id: String!
    name: String!
    category: ProductCategory!
    price: Float
}

input ProductCreateInput {
    name: String!
    category: ProductCategory!
    price: Float!
}

input ProductUpdateInput {
    id: String!
    name: String!
    category: ProductCategory!
    price: Float!
}

input ProductQueryInput {
    id: String
    name: String
    category: ProductCategory
}
